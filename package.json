{
  "name": "adastra",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "@types/node": "^18.11.10",
    "eslint-config-custom": "workspace:*",
    "prettier": "latest",
    "turbo": "latest",
    "typescript": "^5.0.4"
  },
  "engines": {
    "node": ">=14.0.0",
    "pnpm": ">=7.9.5"
  },
  "packageManager": "pnpm@7.14.0"
}
